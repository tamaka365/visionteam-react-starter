{
  "name": "visionteam-react-starter",
  "sideEffects": false,
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "ahooks": "^3.7.0",
    "antd": "^4.22.4",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-app-rewired start --env=dev",
    "start:preview": "react-app-rewired start --env=preview",
    "start:prod": "react-app-rewired start --env=prod",
    "build": "react-app-rewired build --env=prod",
    "build:dev": "react-app-rewired build --env=dev",
    "build:preview": "react-app-rewired build --env=preview",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "serve -s ./build",
    "explore": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.13.1",
    "babel-plugin-import": "^1.13.5",
    "dotenv": "^16.0.1",
    "http-proxy-middleware": "^2.0.6",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.54.4",
    "source-map-explorer": "^2.5.2"
  }
}
